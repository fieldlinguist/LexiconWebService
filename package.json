    "config": "^1.21.0",
    "cors": "^2.8.1",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "forever": "latest",
    "morgan": "^1.7.0",
    "request": "^2.79.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5",
    "supertest": "^2.0.0"
  },
