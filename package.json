    "test:unit": "mocha --recursive test/middleware test/routes",
    "test:integration": "mocha --recursive test/integration",
    "postinstall": "mkdir bin; cd bin; ls Uqailaut.jar || wget https://github.com/nrc-cnrc/InuktitutToolkit/blob/master/Inuktitut-Java/Uqailaut/Uqailaut.jar?raw=true -O Uqailaut.jar"
  },
  "dependencies": {
